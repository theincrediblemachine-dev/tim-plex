#rclone mount --umask 000 --allow-non-empty --allow-other --fast-list --transfers 24 --dir-cache-time 72h --drive-chunk-size=32M --cache-chunk-total-size 8G --cache-chunk-size 8M --cache-chunk-no-memory --cache-workers=24 --buffer-size=32M --vfs-cache-mode minimal --vfs-read-chunk-size 32M --vfs-cache-max-age 1h --vfs-read-chunk-size-limit off --cache-tmp-upload-path=/config/rclone_config GoogleMedia: /mnt/Google --config /config/rclone.cfg &
rclone mount --umask 000 --allow-non-empty --allow-other --fast-list --transfers 24 --dir-cache-time 5000h --attr-timeout 5000h --drive-pacer-min-sleep 10ms --drive-pacer-burst 200 --poll-interval 10s --user-agent plex --vfs-cache-mode full --vfs-read-chunk-size 32M --vfs-cache-max-age 5000h --vfs-cache-max-size 500G --vfs-read-chunk-size-limit off --vfs-cache-poll-interval 5m --vfs-read-ahead 2G --bwlimit-file 32M --cache-dir=/config/rclone_config GoogleMedia: /mnt/Google --config /config/rclone.cfg &
